# v0.19.12

using Markdown
using InteractiveUtils

# ‚ïî‚ïê‚ï° cb7f1920-5efb-11ed-0f14-05c4e836e61c
using Distributions, POMDPs, POMDPModelTools, QuickPOMDPs, QMDP, FIB, PointBasedValueIteration, BasicPOMCP, CSV, DataFrames, BeliefUpdaters

# ‚ïî‚ïê‚ï° 84cfb371-31dd-4d93-8297-0a82da566ef9
begin
	N = 1000; # simulation runs
	T_steps = 100; # decision points
	
	# Rewards
	r_hungry = rand(Distributions.Uniform(-15,-5),N); # original -10
	r_feed = rand(Distributions.Uniform(-7.5,-2.5),N); # original -5 

	# Transition probability
	p_becomes_hungry = 0.1; # original 0.1 FIXED

	# Observation probabilities
	p_crying_when_hungry = rand(Distributions.Uniform(0.4, 0.9),N); # original 0.8
	p_crying_when_full = rand(Distributions.Uniform(0.05, 0.15),N); # original 0.1

	# Discount factor
	discount  = rand(Distributions.Uniform(0.85, 0.95),N); # original 0.9 

	# Output initialization
	output_qmdp = Array{Float64}(undef, N);
	output_fib = Array{Float64}(undef, N);
	output_pomcp = Array{Float64}(undef, N);
end

# ‚ïî‚ïê‚ï° c0f1bd3c-50d5-487e-83c4-4d6d77eedf10
begin
	@enum State hungry full
	@enum Action feed ignore
	@enum Observation crying quiet
end

# ‚ïî‚ïê‚ï° dfa3ebe7-49b9-4919-80db-b76c8f52456b
begin
		p_range = 0:0.001:1;
		dominating_action_idx(policy, ùêõ) = Int(action(policy, ùêõ))+1;
		dominant_actions(policy) = map(p->
		dominating_action_idx(policy,[p,1-p]), p_range);
end

# ‚ïî‚ïê‚ï° a991dd6b-34b7-42e8-9636-48041fcbac5d
import POMDPSimulators: HistoryRecorder

# ‚ïî‚ïê‚ï° 47b2519a-4ca9-4528-89b4-1d6924ec8ae9
for i in 1:N

	# POMDP
    
			pomdp = QuickPOMDP(
		    states       = [hungry, full],  # ùíÆ
		    actions      = [feed, ignore],  # ùíú
		    observations = [crying, quiet], # ùí™
		    initialstate = [full],          # Deterministic initial state
		    
		
		    transition = function T(s, a)
		        if a == feed
		            return SparseCat([hungry, full], [0, 1])
		        elseif s == hungry && a == ignore
		            return SparseCat([hungry, full], [1, 0])
		        elseif s == full && a == ignore
		            return SparseCat([hungry, full], [p_becomes_hungry, 1 - p_becomes_hungry])
		        end
		    end,
		
		    observation = function O(s, a, s‚Ä≤)
		        if s‚Ä≤ == hungry
		            return SparseCat([crying, quiet], [p_crying_when_hungry[i], 1 - p_crying_when_hungry[i]])
		        elseif s‚Ä≤ == full
		            return SparseCat([crying, quiet], [p_crying_when_full[i], 1 - p_crying_when_full[i]])
		        end
		    end,
		
		    reward = (s,a)->(s == hungry ? r_hungry[i] : 0) + (a == feed ? r_feed[i] : 0)
		)

		updater = DiscreteUpdater(pomdp);
		b0 = uniform_belief(pomdp);
		recorder = HistoryRecorder(max_steps=T_steps);
	
	# solve POMDP
	
		# QMDP	
		qmdp_solver = QMDPSolver();
		qmdp_policy = solve(qmdp_solver, pomdp);
		thresh_qmdp = p_range[findfirst(dominant_actions(qmdp_policy) .== 1)];

		# FIB
		fib_solver = FIBSolver();
		fib_policy = solve(fib_solver, pomdp);
		thresh_fib = p_range[findfirst(dominant_actions(fib_policy) .== 1)];

		#POMCP
		pomcp_solver = POMCPSolver()
		pomcp_planner = solve(pomcp_solver, pomdp);
		history_pomcp = simulate(recorder,
		               pomdp,
		               pomcp_planner,
		               updater,
					   b0,
	                   rand(initialstate(pomdp)))
		b_count = Array{Float64}(undef, T_steps).*0;
		b_sum = Array{Float64}(undef, T_steps).*0;
		for k in 1 : T_steps
			if  history_pomcp[k].a == feed
				b_count[k] = 1;
				b_sum[k] = history_pomcp[k].b.b[1];
			end
		end

		thresh_pomcp = sum(b_sum)/sum(b_count);
	
	# OUTPUT
	
	output_qmdp[i] = thresh_qmdp;
	output_fib[i] = thresh_fib;
	output_pomcp[i] = thresh_pomcp;
	
end

# ‚ïî‚ïê‚ï° a4e6fa64-1098-4534-a35f-88cf444eb9b2
df_qmdp = DataFrame([output_qmdp r_hungry r_feed p_crying_when_hungry p_crying_when_full discount], :auto)


# ‚ïî‚ïê‚ï° 27bc7dff-6719-4069-9399-927e531e7a7f
df_fib = DataFrame([output_fib r_hungry r_feed p_crying_when_hungry p_crying_when_full discount], :auto)


# ‚ïî‚ïê‚ï° 8840a552-2ae7-42da-9db0-4abba7991728
df_pomcp = DataFrame([output_pomcp r_hungry r_feed p_crying_when_hungry p_crying_when_full discount], :auto)

# ‚ïî‚ïê‚ï° 8b2239e7-39c4-4a85-a268-012b5c791b7f
begin
	CSV.write("Desktop\\2qmdp.csv", df_qmdp)
	CSV.write("Desktop\\2fib.csv", df_fib)
	CSV.write("Desktop\\2pomcp.csv", df_pomcp)
end

